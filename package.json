{
  "name": "tailwindcss-plugin-starter",
  "version": "0.0.0",
  "description": "A short description of the plugin",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc --watch & cd ./demo && npm run-script dev",
    "build": "tsc",
    "release": "np",
    "test": "tsc && c8 jest"
  },
  "license": "MIT",
  "repository": "https://github.com/maizzle/tailwindcss-plugin-starter",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "c8": "^7.12.0",
    "jest": "^28.1.3",
    "np": "^7.6.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.1.0"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest/customMatchers.js"
    ]
  },
  "c8": {
    "exclude": [
      "jest/customMatchers.js",
      "**/*.test.js"
    ]
  },
  "keywords": [
    "tailwindcss"
  ]
}
